            <Button Content="US" Margin="5" Width="50" FontSize="20" Background="Azure" FontFamily="Rockwell" BorderBrush="DarkRed" BorderThickness="3" Click="US_Click"/>
            <Button Content="GB" Margin="5" Width="50" FontSize="20" Background="Azure" FontFamily="Rockwell" BorderBrush="DarkBlue" BorderThickness="3" Click="GB_Click"/>
            <Button Content="DE" Margin="5 5 10 5" Width="50" FontSize="20" Background="Azure" FontFamily="Rockwell" BorderBrush="DarkGoldenrod" BorderThickness="3" Click="DE_Click"/>
