        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{Binding ElementName=Title, Path=Text}" Height="600" Width="520" ResizeMode="CanMinimize" Icon="C:\Users\User\documents\visual studio 2012\Projects\News BG\News BG\Assets\THE NEWS ICON.png">
